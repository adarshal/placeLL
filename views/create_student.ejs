<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
<link rel="stylesheet" href="/css/users.css">

<form action="./createform/student" method="POST">
 

    <div class="form-group mb-4">
        <label for="name" class="form-label">Student Name :</label>
        <input type="text" class="form-control" name="name">
      </div>
      <div class="form-group">
        <label for="batch">Batch:</label>
        <input type="text" class="form-control" name="batch">
      </div>
      <div class="form-group">
        <label for="college">College :</label>
        <input type="text" class="form-control" name="college">
      </div>
      <div class="form-group">
        <label for="dsa">DSA marks :</label>
        <input type="number" class="form-control" name="dsa" min="0" max="100">
      </div>
      <div class="form-group">
        <label for="webd">webd score:</label>
        <input type="number" class="form-control" name="webd" min="0" max="100">
      </div>
      <div class="form-group">
        <label for="react">react score :</label>
        <input type="number" class="form-control" name="react" min="0" max="100">
      </div>
        
      <!-- <div class="form-group">
        <label for="income">score :</label>
        <input type="number" class="form-control" name="income">
      </div> -->
      <div class="form-group">
        <input class="form-check-input" type="radio" name="placed" value="1" id="flexRadioDefault1">
        <label class="form-check-label" for="flexRadioDefault1">
         placed
        </label>
      </div>
      <div class="form-group">
        <input class="form-check-input" type="radio" name="placed" value="0" id="flexRadioDefault2" checked>
        <label class="form-check-label" for="flexRadioDefault2">
           not placed
        </label>
      </div>
      <div class="form-group">
        <button type="submit" class="btn" >Submit</button>
</div>
</form>

